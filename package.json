{
  "version": "0.1.0",
  "name": "jemul8",
  "publishConfig": {
    "proprietary-attribs": false
  },
  "description": "An object-oriented JavaScript x86 Emulator",
  "keywords": [
    "emulator",
    "x86",
    "intel",
    "amd"
  ],
  "config": {
    "publishtest": false
  },
  "homepage": "http://jemul8.com/",
  "author": "Daniel Phillimore <dan@ovms.co> (https://ovms.co)",
  "repository": {
    "type": "git",
    "url": "https://github.com/asmblah/jemul8"
  },
  "bugs": {
    "email": "dan@ovms.co",
    "url": "https://github.com/asmblah/jemul8/issues"
  },
  "bin": {
    "jemul8": "bin/jemul8",
    "jemul8-server": "bin/server"
  },
  "scripts": {
    "build:release": "cd dist && webpack -p",
    "build:debug": "cd dist && webpack",
    "build:demo:debug": "webpack --config demos/floppy/webpack.config --devtool source-map",
    "build:demo:watch": "webpack --config demos/floppy/webpack.config --devtool source-map -w",
    "build:server:debug": "webpack --config webpack.config-server --devtool source-map",
    "server": "npm run build:server:debug && node bin/server",
    "standard": "standard",
    "standard:fix": "standard --fix",
    "test:browser:build": "npm run test:browser:server:build && npm run test:browser:client:build",
    "test:browser:server:build": "webpack --config tests/bdd/harness/webpack.config-browser-server.js",
    "test:browser:client:build": "webpack --config tests/bdd/harness/webpack.config-browser-client.js",
    "test:browser:server": "node tests/bdd/server",
    "test:mocha": "mocha-webpack --webpack-config tests/bdd/harness/webpack.config-node.js --include tests/bdd/harness/node -r source-map-support/register 'tests/bdd/**/*Test.js'",
    "test:jest": "jest tests/jest",
    "test": "npm run test:jest && npm run test:mocha",
    "webtest": "node tests/bdd/webtest.js"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "node-getopt": "0.2.x",
    "wabt": "0.0.13",
    "ws": "0.4.*"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-amd-to-commonjs": "^0.2.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.3",
    "chai": "1.7.x",
    "express": "^4.16.3",
    "glob": "^7.1.3",
    "inject-loader": "^3.0.1",
    "jest-cli": "^20.0.4",
    "mocha": "^3.5.0",
    "mocha-webpack": "^0.7.0",
    "setimmediate": "^1.0.5",
    "sinon": "^3.2.0",
    "sinon-chai": "2.4.x",
    "source-map-loader": "^0.2.1",
    "source-map-support": "^0.4.17",
    "standard": "^10.0.3",
    "tmp": "0.0.33",
    "webpack": "^3.5.5",
    "webpack-node-externals": "^1.6.0"
  },
  "engines": {
    "node": ">=0.6",
    "npm": "1"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/asmblah/jemul8/raw/master/MIT-LICENSE.txt"
    }
  ],
  "jest": {
    "clearMocks": true
  },
  "standard": {
    "globals": [
      "afterEach",
      "beforeEach",
      "describe",
      "expect",
      "test"
    ]
  }
}
